{
  "version": 2,
  "builds": [
    { "src": "api/*.py", "use": "@vercel/python" },
    { "src": "static/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/app.py" },

    { "src": "/admin", "dest": "/static/admin.html" },
    { "src": "/miniapp", "dest": "/static/miniapp.html" },

    { "src": "/static/(.*)", "dest": "/static/$1" },

    { "src": "/(.*)", "dest": "/static/miniapp.html" }
  ]
}
